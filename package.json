{
    "name": "yatasks_one_api",
    "version": "1.0.0",
    "private": true,
    "description": "A very simple back-end for small sites.",
    "main": "index.js",
    "sideEffects_DISABLED": [],
    "scripts": {
        "start": "node ts_out\\src\\start.js",
        "test": "jest",
        "watch": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline -w",
        "nodemon": "nodemon ts_out\\src\\start.js --devuser=dev",
        "build_old": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline",
        "build": "npm run build:clean && npm run build:ts",
        "build:ts": "npx rollup -c",
        "build:clean": "rm -rf ./dist/*",
        "ycplmon": "node ..\\ycplmon_amaglion\\ts_out\\src\\ycplmon.js src",
        "tsc": "tsc",
        "deps": "echo Finding loops in .js requires... & del deps.png & madge ts_out/src -c -i deps.png && deps.png",
        "deps_all": "echo Generating full .js requires tree... & del deps.png & madge ts_out/src -i deps.png && deps.png",
        "deps_orphans": "echo Finding orphans .js requires... & del deps.png & madge ts_out/src --orphans -i deps.png && deps.png",
        "deploy": "pm2 stop yatasks & git checkout . && git pull && pnpm i && npm run build && pm2 start yatasks"
    },
    "author": "Yuri Yaryshev",
    "license": "Unlicense",
    "jest": {
        "rootDir": "ts_out"
